{
	"name": "stencilunittests",
	"version": "0.0.1",
	"description": "Repo to demonstrate issue with StencilJS unit test runner",
	"main": "dist/index.cjs.js",
	"module": "dist/index.js",
	"es2015": "dist/esm/index.mjs",
	"es2017": "dist/esm/index.mjs",
	"types": "dist/types/components.d.ts",
	"collection": "dist/collection/collection-manifest.json",
	"collection:main": "dist/collection/index.js",
	"unpkg": "dist/stencilunittests/stencilunittests.esm.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Kwooda/StencilUnitTests.git"
	},
	"bugs": {
		"url": "https://github.com/Kwooda/StencilUnitTests/issues"
	},
	"homepage": "https://github.com/Kwooda/StencilUnitTests#readme",
	"files": [
		"dist/",
		"loader/"
	],
	"scripts": {
		"build": "stencil build --docs",
		"start": "stencil build --dev --watch --serve",
		"test": "npx stencil test --spec --e2e --coverage",
		"test.watch": "stencil test --spec --e2e --watchAll",
		"generate": "stencil generate"
	},
	"devDependencies": {
		"@stencil/angular-output-target": "0.8.0",
		"@stencil/core": "4.0.5",
		"@stencil/react-output-target": "0.5.3",
		"@stencil/sass": "3.0.5",
		"@types/jest": "^27.0.3",
		"@types/puppeteer": "7.0.4",
		"jest": "^27.4.5",
		"jest-cli": "^27.4.5",
		"puppeteer": "^10.0.0",
		"typescript": "4.6.2"
	},
	"license": "MIT"
}
